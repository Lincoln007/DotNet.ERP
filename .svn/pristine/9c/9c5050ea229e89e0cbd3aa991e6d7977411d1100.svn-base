using Pharos.POS.Retailing.ChildWin;
using Pharos.Wpf.HotKeyHelper;
using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using Pharos.POS.Retailing.ChildWin;

namespace Pharos.POS.Retailing.Models.HotKeyCommand
{
    public class MemberCommand : IHotKeyCommand
    {
        public Action<System.Windows.Window> Handler
        {
            get
            {
                return new Action<System.Windows.Window>((o) =>
                {
                    MemberEditor page = new MemberEditor();
                    page.Owner = o;
                    page.ShowDialog();
                });
            }
        }
    }
}
