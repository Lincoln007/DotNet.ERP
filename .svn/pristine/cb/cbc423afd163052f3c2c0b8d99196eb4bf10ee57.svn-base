<control:DialogWindow02 x:Class="Pharos.POS.Retailing.ChildWin.ZhengDanZheKou"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:converter="clr-namespace:Pharos.Wpf.XamlConverters;assembly=Pharos.Wpf"       
         xmlns:control="clr-namespace:Pharos.Wpf.Controls;assembly=Pharos.Wpf"
        Title="日结" Height="350" Width="400" WindowStartupLocation="CenterScreen" Icon="..\Images\ChaKuCun\search.png" Margin="0" WindowStyle="None">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="..\Styles\ZhengDanZheKouStyle.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="140"></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="50"></RowDefinition>
            <RowDefinition Height="50"></RowDefinition>
            <RowDefinition Height="50"></RowDefinition>
            <RowDefinition Height="50"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <TextBlock Style="{StaticResource titleStyle}" Text="应收金额："></TextBlock>
        <control:IconTextBox Grid.Row="0" Grid.Column="1" Style="{StaticResource contentStyle}"></control:IconTextBox>
        <TextBlock Grid.Row="1" Style="{StaticResource titleStyle}" Text="折扣（0-10）："></TextBlock>
        <control:IconTextBox Grid.Row="1" Grid.Column="1" Style="{StaticResource contentStyle}"></control:IconTextBox>
        <TextBlock Grid.Row="2" Style="{StaticResource titleStyle}" Text="折后金额："></TextBlock>
        <control:IconTextBox Grid.Row="2" Grid.Column="1" Style="{StaticResource contentStyle}"></control:IconTextBox>
        <TextBlock Grid.Row="3" Style="{StaticResource titleStyle}" Text="授权密码："></TextBlock>
        <control:IconPasswordBox Grid.Row="3" Grid.Column="1" Style="{StaticResource IconPasswordBoxStyle}"></control:IconPasswordBox>

        <control:PosButton  Grid.Row="4" Grid.ColumnSpan="2"   Style="{StaticResource BtnSearchStyle}" Content="确定，并保存"></control:PosButton>
    </Grid>
</control:DialogWindow02>

