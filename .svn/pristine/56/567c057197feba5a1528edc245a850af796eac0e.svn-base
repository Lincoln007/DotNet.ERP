using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace Pharos.Logic.OMS.Entity
{
    public class Realm
    {
        public int Id { get; set; }
        /// <summary>
        /// -1-保留域名
        /// </summary>
        public int CID { get; set; }
        /// <summary>
        /// 域名前缀
        /// </summary>
        public string Name { get; set; }
        /// <summary>
        /// 跳转地址
        /// </summary>
        public string Url { get; set; }
        /// <summary>
        /// 可用状态(1-可用,0-禁用)
        /// </summary>
        public short State { get; set; }
    }
}
