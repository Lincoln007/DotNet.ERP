using Pharos.POS.Retailing.ChildWin;
using Pharos.POS.Retailing.Models.ViewModels;
using Pharos.Wpf.HotKeyHelper;
using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Windows;

namespace Pharos.POS.Retailing.Models.HotKeyCommands
{
    /// <summary>
    /// 多方式支付快捷键
    /// </summary>
    public class DuoFangShiZhiFuCommand : IHotKeyCommand
    {
        public Action<Window> Handler
        {
            get
            {
                return new Action<Window>((win) =>
                {
                    DuoFangShiZhiFu page = new DuoFangShiZhiFu(PosViewModel.Current.Preferential, PosModels.PayAction.Sale);
                    page.Owner = win;
                    page.ShowDialog();
                });
            }
        }
    }
}
