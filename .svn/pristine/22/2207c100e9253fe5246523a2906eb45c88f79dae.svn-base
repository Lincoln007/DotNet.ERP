<control:DialogWindow x:Class="Pharos.POS.Retailing.MachineSettings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:control="clr-namespace:Pharos.Wpf.Controls;assembly=Pharos.Wpf"
        Title="系统设置"  Height="550" Width="800" WindowStartupLocation="CenterScreen" Icon="Images\Setting.png" Margin="0" WindowStyle="None">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles\MachineSettingsStyle.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>

    </Window.Resources>
    <control:OfficeTabControl x:Name="tabControl" ItemTemplateSelector="{DynamicResource DefaultTabHeaderTemplateSelector}" ContentTemplateSelector="{DynamicResource MachineSettingsPageTemplateSelector}"></control:OfficeTabControl>
</control:DialogWindow>
