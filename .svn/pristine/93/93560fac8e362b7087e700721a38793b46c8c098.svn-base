@model Pharos.Logic.OMS.Entity.Business
@{
    Layout = "~/Views/Shared/_FormLayout.cshtml";    
}

<div class="default-form">
    @using (Html.BeginForm())
    {
        @Html.HiddenFor(o=>o.Id)
        @Html.HiddenFor(o=>o.ById)
        <div class="content">
            <table class="table-form" width="100%" height="117px">
                <tr>
                    <td class="name">行业名称：</td>
                    <td class="input">@Html.TextBoxFor(o => o.Title, new { @class = "easyui-textbox", data_options = "required:true" })</td>
                </tr>
                <tr>
                    <td class="name">行业别名：</td>
                    <td class="input">@Html.TextBoxFor(o => o.Byname, new { @class = "easyui-textbox", data_options = "" })</td>
                </tr>
            </table>
        </div>
    }
</div>



