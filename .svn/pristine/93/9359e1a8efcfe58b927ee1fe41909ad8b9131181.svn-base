[Version]
Signature = "$Windows NT$"
Provider = %CorpName%
Class = Printer
ClassGUID = {4D36E979-E325-11CE-BFC1-08002BE10318}
CatalogFile=Oemsetup.cat

; Date & version of driver package
DriverVer=11/11/2014,1.1.2.1

[DestinationDirs]
DefaultDestDir = 66000

[Manufacturer]
%CorpName% = ZMIN,NTamd64


;[ZMIN]
;"ZMIN X1"	      = ZMINX1.GPD,ZMIN_X1E0C6
;"ZMIN X1i"	      = ZMINX1i.GPD,ZMIN_X1i7CA0

[ZMIN.NTamd64]
"ZMIN X1"	      = ZMINX1.GPD,USBPRINT\ZMIN_X1E0C6,ZMIN_X1E0C6
"ZMIN X1i"	      = ZMINX1i.GPD,USBPRINT\ZMIN_X1i7CA0,ZMIN_X1i7CA0
"ZMIN X100"	      = ZMINX1.GPD,USBPRINT\ZMIN_X1E0C6,ZMIN_X1E0C6
"ZMIN X100i"	      = ZMINX1i.GPD,USBPRINT\ZMIN_X1i7CA0,ZMIN_X1i7CA0
"ZMIN EasyBar 4e"     = ZMINX1.GPD,USBPRINT\ZMIN_X1E0C6,ZMIN_X1E0C6
"ZMIN EasyBar 4i"     = ZMINX1i.GPD,USBPRINT\ZMIN_X1i7CA0,ZMIN_X1i7CA0
"ZMIN X1-200"	      = ZMINX1.GPD,USBPRINT\ZMIN_X1E0C6,ZMIN_X1E0C6
"ZMIN X1i-300"	      = ZMINX1i.GPD,USBPRINT\ZMIN_X1i7CA0,ZMIN_X1i7CA0
"ZMIN X16"	      = ZMINX1.GPD,USBPRINT\ZMIN_X1646E0,ZMIN_X1646E0
"ZMIN X16i"	      = ZMINX1i.GPD,USBPRINT\ZMIN_X16iA687,ZMIN_X16iA687

[ZMINX1.GPD]
CopyFiles=@ZMPWD2.dll,@ZMINX1.GPD,@ZMPWDUI.INI,ZMPWDUNI,ZMPWDUI,MONITOR_USB,@ZMLANMON.DLL
DataFile = ZMINX1.GPD
DataSection=UNIDRV_DATA
Include=NTPRINT.INF
LanguageMonitor=%LAN_MONITOR%
Needs=UNIDRV.OEM
PortMonitor="Microsoft USB Port Monitor",usbmon.dll

[ZMINX1i.GPD]
CopyFiles=@ZMPWD3.dll,@ZMINX1i.GPD,@ZMPWDUI.INI,ZMPWDUNI,ZMPWDUI,MONITOR_USB,@ZMLANMON.DLL
DataFile = ZMINX1i.GPD
DataSection=UNIDRV_DATA
Include=NTPRINT.INF
LanguageMonitor=%LAN_MONITOR%
Needs=UNIDRV.OEM
PortMonitor="Microsoft USB Port Monitor",usbmon.dll

;
; CopyFiles Sections
;
[ZMPWDUNI]
ZMPWDUNI.DLL

[ZMPWDUI]
ZMPWDUI.DLL

[MONITOR_USB]
usbmon.dll,,,16

;
; Data Sections
; These sections contain data that is shared between devices
;
[UNI_DATA]
;HelpFile=UNIDRV.HLP
;DefaultDataType=EMF


;[SourceDisksNames.x86]
;1 = %disk1%,,,""

[SourceDisksNames.amd64]
1 = %disk1%,,,""

[SourceDisksFiles]
ZMINX1.GPD     = 1
ZMINX1i.GPD     = 1

ZMPWD2.DLL	= 1
ZMPWD3.DLL	= 1

ZMPWDUI.DLL     =  1
ZMPWDUNI.DLL    = 1
ZMPWDUI.INI     = 1

USBMON.DLL      = 1
USBPRINT.SYS    = 1


ZMLANMON.DLL    = 1
;
; Localizable strings
;
[Strings]
;MS="Microsoft"
CorpName="ZMIN"
disk1="ZMIN Printer Driver Setup Disk"
LAN_MONITOR="ZMIN Language Monitor,ZMLANMON.DLL"

