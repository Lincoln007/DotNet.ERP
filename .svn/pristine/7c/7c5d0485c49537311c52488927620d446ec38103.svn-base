@{
    ViewBag.Title = Pharos.Sys.SysConstPool.Page_Title;
    Layout = "~/Views/Shared/_FormLayout.cshtml";
}
<div class="default-form">
    @using (Html.BeginForm())
    {
        @Html.Hidden("Id")
        <div class="content">
            <table class="table-form" width="100%" height="150px">
                <tr>
                    <td class="name">盘点门店：</td>
                    <td class="input">@Html.DisplayText("StoreTitle")</td>
                    <td class="name">盘点员：</td>
                    <td class="input">@Html.DisplayText("FullName")</td>
                </tr>
                <tr>
                    <td class="name">商品条码：</td>
                    <td class="input">@Html.DisplayText("Barcode")</td>
                    <td class="name">品 名：</td>
                    <td class="input">@Html.DisplayText("Title")</td>
                </tr>
                <tr>
                    <td class="name">锁库库存：</td>
                    <td class="input">@Html.DisplayText("LockNumber")</td>
                    <td class="name">实盘数量：</td>
                    <td class="input">@Html.DisplayText("ActualNumber")</td>
                </tr>
                <tr>
                    <td class="name">库存纠正数量：</td>
                    <td class="input" colspan="3">@Html.TextBox("CrrectNumber", ViewData.Eval("CrrectNumber"), new { @class = "easyui-numberspinner", data_options = "min:0,width:100,required:true" })</td>
                    <td class="name"></td>
                    <td class="input"></td>
                </tr>
            </table>
    </div>
    }
</div>