using System;

namespace Pharos.Logic.OMS.Entity
{
	/// <summary>
	/// 代理商帐号
	/// </summary>
	[Serializable]
	public class AgentsUsers
	{
		/// <summary>
		/// 记录ID
		/// [主键：√]
		/// [长度：10]
		/// [不允许为空]
		/// </summary>
		public int Id
		{
			get { return _Id; }
			set { _Id = value; }
		}
		private int _Id;

		/// <summary>
		/// 代理商编号
		/// [长度：10]
		/// [不允许为空]
		/// </summary>
		public int AgentsId
		{
			get { return _AgentsId; }
			set { _AgentsId = value; }
		}
		private int _AgentsId;

		/// <summary>
		/// 帐号GUID
		/// [长度：40]
		/// [不允许为空]
		/// </summary>
		public string AgentsLoginId
		{
			get { return _AgentsLoginId; }
			set { _AgentsLoginId = value; }
		}
		private string _AgentsLoginId;

		/// <summary>
		/// 登录帐号
		/// [长度：10]
		/// [不允许为空]
		/// </summary>
		public int LoginName
		{
			get { return _LoginName; }
			set { _LoginName = value; }
		}
		private int _LoginName;

		/// <summary>
		/// 登录密码
		/// [长度：5]
		/// [不允许为空]
		/// </summary>
		public short LoginPwd
		{
			get { return _LoginPwd; }
			set { _LoginPwd = value; }
		}
		private short _LoginPwd;

		/// <summary>
		/// 员工姓名
		/// [长度：10]
		/// [不允许为空]
		/// </summary>
		public int FullName
		{
			get { return _FullName; }
			set { _FullName = value; }
		}
		private int _FullName;

		/// <summary>
		/// 账号类型（1是主账户，2是普通账户）
		/// [长度：10]
		/// [不允许为空]
		/// </summary>
		public int AgentType
		{
			get { return _AgentType; }
			set { _AgentType = value; }
		}
		private int _AgentType;

		/// <summary>
		/// 操作权限
		/// [长度：2000]
		/// </summary>
		public string Limits
		{
			get { return _Limits; }
			set { _Limits = value; }
		}
		private string _Limits;

		/// <summary>
		/// 创建时间
		/// [长度：23，小数位数：3]
		/// [不允许为空]
		/// [默认值：(getdate())]
		/// </summary>
		public DateTime CreateTime
		{
			get { return _CreateTime; }
			set { _CreateTime = value; }
		}
		private DateTime _CreateTime;

		/// <summary>
		/// 最新登录
		/// [长度：23，小数位数：3]
		/// </summary>
		public DateTime LatestLogin
		{
			get { return _LatestLogin; }
			set { _LatestLogin = value; }
		}
		private DateTime _LatestLogin;

		/// <summary>
		/// 帐号状态（0:停用，1:正常）
		/// [长度：10]
		/// [不允许为空]
		/// </summary>
		public int Status
		{
			get { return _Status; }
			set { _Status = value; }
		}
		private int _Status;

		/// <summary>
		/// 备注
		/// [长度：200]
		/// </summary>
		public string Memo
		{
			get { return _Memo; }
			set { _Memo = value; }
		}
		private string _Memo;
	}
}
