<Page x:Class="Pharos.POS.Retailing.Pages.LockPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" ShowsNavigationUI="False"
      mc:Ignorable="d" 
      d:DesignHeight="768" d:DesignWidth="1024" Foreground="#FAFAFA	" FontSize="20"
	Title="LockPage">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"></ColumnDefinition>
            <ColumnDefinition Width="Auto"></ColumnDefinition>
            <ColumnDefinition Width="Auto"></ColumnDefinition>
            <ColumnDefinition Width="*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="Auto"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <Border Background="#363636" Opacity="0.5" Grid.ColumnSpan="4" Grid.RowSpan="3"></Border>
        
        
        <Image Grid.Column="1" Grid.Row="1" Source="..\Images\PosWindow\Lock.png" Width="200" Margin="20,0"></Image>
        <StackPanel Grid.Column="2" Grid.Row="1" VerticalAlignment="Center">
            <StackPanel Orientation="Horizontal" Margin="10">
                <TextBlock Text="收 银 员 ：" Width="100"></TextBlock>
                <TextBlock Text="李四"></TextBlock>
            </StackPanel>
            <StackPanel  Orientation="Horizontal" Margin="10">
                <TextBlock Text="锁屏时间："  Width="100"></TextBlock>
                <TextBlock Text="11:30:06"></TextBlock>
            </StackPanel>
            <StackPanel  Orientation="Horizontal" Margin="10">
                <TextBlock Text="系统时间："  Width="100"></TextBlock>
                <TextBlock Text="2016/02/11 11:39:07"></TextBlock>
            </StackPanel>

        </StackPanel>
    </Grid>
</Page>
