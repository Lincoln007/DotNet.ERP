@using Pharos.Logic.OMS.Entity
@using Pharos.Logic.OMS.Entity.View
@model  Traders
@{
    ViewBag.Title = "Save";
    Layout = "~/Views/Shared/_FormLayout.cshtml";
}

<style type="text/css">
    .formbox td {
        padding: 5px 0px;
    }
</style>
<script src="~/Scripts/tool.js"></script>
<script type="text/javascript">

    $(function () {

    });

    function SaveBefore() {
        return true;
    }
</script>

<div class="default-form">
    @using (Html.BeginForm())
    {

        <div class="content">

            <div class="formbox tab3" >

                <table class="table-form" width="100%">

                    <tr>
                        <td class="name">指派人：</td>
                        <td class="input">@Html.DropDownListFor(o => o.Source, ViewBag.user as List<SelectListItem>, new { @class = "easyui-combobox datacontrol", data_options = "required:true,novalidate:true,validType:'requiredForCombo', editable: false,width:120" })</td>
                        <td class="name" style="width:10%;">商户号：</td>
                        <td class="input">@Html.TextBoxFor(o => o.Title, new { @class = "easyui-textbox", data_options = "required:true,prompt:'',width:120" })</td>
                        <td class="name">客户全称：</td>
                        <td class="input">@Html.TextBoxFor(o => o.Title, new { @class = "easyui-textbox", data_options = "required:false,prompt:'',width:120,readonly:true" })</td>
                    </tr>

                    <tr>
                        <td class="name">第三方平台商户号：</td>
                        <td class="input">@Html.TextBoxFor(o => o.Title, new { @class = "easyui-textbox", data_options = "required:false,prompt:'',width:120,validType:'length[0,100]'" })</td>
                        <td class="name">第三方平台Key：</td>
                        <td class="input" colspan="3">@Html.TextBoxFor(o => o.Cities, new { @class = "easyui-textbox", data_options = "required:false,prompt:'',width:469,validType:'length[0,200]'" })</td>
                    </tr>

                    <tr>
                        <td class="name"></td>
                        <td class="input"></td>
                        <td class="name">签名类型：</td>
                        <td class="input" colspan="3">@Html.DropDownList("keywordType", new List<SelectListItem>() { new SelectListItem() { Text = "请选择", Value = "0" }, new SelectListItem() { Text = "MD5", Value = "1" } }, new { @class = "easyui-combobox datacontrol", data_options = "required:true,novalidate:true, editable: false,width:120,validType:'requiredForCombo'" })</td>
                    </tr>

                </table>

            </div>

            <div class="formbox tab3" style="padding-top:10px;">

                <table class="table-form" width="100%">

                    <tr>
                        <td class="name" style="width:134px;">支付终端：</td>
                        <td class="input" style="width:150px;">@Html.DropDownListFor(o => o.Source, ViewBag.user as List<SelectListItem>, new { @class = "easyui-combobox datacontrol", data_options = "required:true,novalidate:true,validType:'requiredForCombo', editable: false,width:120" })</td>
                        <td class="name" style="width:94px;">支付通道：</td>
                        <td class="input">@Html.DropDownListFor(o => o.Source, ViewBag.user as List<SelectListItem>, new { @class = "easyui-combobox datacontrol", data_options = "required:true,novalidate:true,validType:'requiredForCombo', editable: false,width:120" })</td>
                    </tr>

                </table>

            </div>

        </div>
    }
</div>


