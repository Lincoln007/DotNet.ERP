    //tab切换
    $(".tab-menu a").click(function () {
        //点击的a标签添加selected样式，然后把同级兄弟节点移除selected样式
        $(this).addClass("selected").siblings().removeClass("selected");
        //获取点击的a标签在菜单中的索引，就是在第几个
        var index = $(this).index(".tab-menu a");
        //将内容对应的第几个div显示，其他兄弟节点隐藏
        $(".tab-content>table").eq(index).show().siblings().hide();
    });


</script>

