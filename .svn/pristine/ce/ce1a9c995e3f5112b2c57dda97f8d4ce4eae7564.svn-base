            this.DeviceId = req.DeviceId == -1 ? "" : req.DeviceId.ToString();
            this.SignType = PayConst.SXF_DEF_SIGNTYPE;
            this.Version = PayConst.SXF_DEF_VERSION;
            this.Type = PayConst.SXF_TYPE_PAYORDERPAGEQUERY;
            this.PageNum = req.PageNum;
            this.PageSize = req.PageSize;
            this.StartDate = req.StartDate;
            this.EndDate = req.EndDate;
