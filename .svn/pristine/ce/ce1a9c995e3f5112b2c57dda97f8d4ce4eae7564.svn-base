    /// <summary>
    /// 单笔支付订单查询响应参数
    /// </summary>
    [Serializable]
    public class SxfPayBatchQueryResponse : SxfBaseTradeResponse
    {
        #region Properties
        /// <summary>
        /// 查询起始日期，格式yyyyMMdd
        /// </summary>
        [JsonProperty("startDate")]
        public string StartDate { get; set; }
        /// <summary>
        /// 截止日期，格式yyyyMMdd
        /// </summary>
        [JsonProperty("endDate")]
        public string EndDate { get; set; }
        /// <summary>
        /// 订单类型
        /// </summary>
        [JsonProperty("orderType")]
        public string OrderType { get; set; }
        /// <summary>
        /// 用户手机号(what? 批量为何要userMobile)
        /// </summary>
        [JsonProperty("userMobile")]
        public string BuyerMobile { get; set; }
        /// <summary>
        /// 页序号
        /// </summary>
        [JsonProperty("PAG_NO")]
        public int PageNum { get; set; }
        /// <summary>
        /// 每页记录数
        /// </summary>
        [JsonProperty("PER_PAG_CNT")]
        public int PageSize { get; set; }
        /// <summary>
        /// 商户批量查询订单List
        /// </summary>
        [JsonProperty("orderList")]
        public List<SxfBasePayQueryResponse> OrderList { get; set; }
        #endregion

        #region Methods
        /// <summary>
        /// 转换为PayBatchQueryResponse
        /// </summary>
        /// <param name="merchStore"></param>
        /// <returns></returns>
        public PayBatchQueryResponse ToPayBatchQueryRsp(MerchantStore merchStore)
        {
            var obj = new PayBatchQueryResponse()
            {
                BuyerMobile = this.BuyerMobile,
                CID = merchStore.CID,
                DeviceId = string.IsNullOrEmpty(this.DeviceId) ? 0 : int.Parse(this.DeviceId),
                OrderType = this.OrderType,
                ReturnCode = this.ReturnCode,
                ReturnMsg = this.ReturnMsg,
                SID = merchStore.SID,
                SignType = this.SignType,
                Version = PayConst.DEF_VERSION,
                StartDate = this.StartDate,
                EndDate = this.EndDate,
                PageNum = this.PageNum,
                PageSize = this.PageSize,
                OrderList = this.ToBasePayQueryRspOrderList(merchStore)
            };
            return obj;
        }
        /// <summary>
        /// 转换为BasePayQueryOrderList
        /// </summary>
        /// <param name="merchStore"></param>
        /// <returns></returns>
        public List<BasePayQueryResponse> ToBasePayQueryRspOrderList(MerchantStore merchStore)
        {
            var orderList = new List<BasePayQueryResponse>();
            if (this.OrderList != null)
            {
                foreach (var item in this.OrderList)
                {
                    orderList.Add(new BasePayQueryResponse()
                    {
                        BuyerMobile = item.BuyerMobile,
                        OrderType = item.OrderType,
                        OutTradeNo = item.OutTradeNo,
                        PayChannel = item.PayChannel,
                        PayDate = PayHelper.Convert2DateFormat(item.PayDate, item.PayTime, "yyyy-MM-dd HH:mm:ss"),
                        PayStatus = (PayHelper.Convert2EnumString<PayStatus>(PayHelper.Convert2EnumValue<SxfPayStatus>(item.PayStatus))).ToUpper(),
                        TotalAmount = item.TotalAmount
                    });
                }
            }
            return orderList;
        }
        #endregion
    }

