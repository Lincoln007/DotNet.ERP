    public class SxfRefundQueryRequest : SxfBaseTradeRequest
    {
        /// <summary>
        /// 构造函数
        /// </summary>
        /// <param name="req"></param>
        /// <param name="merchStore"></param>
        public SxfRefundQueryRequest(RefundQueryRequest req, MerchantStore merchStore)
        {
            this.CharSet = PayConst.SXF_DEF_CHARSET;
            this.MchId = merchStore.MchId3;
            this.StoreId = merchStore.StoreId3;
            this.DeviceId = req.DeviceId.ToString();
            this.SignType = PayConst.SXF_DEF_SIGNTYPE;
            this.Type = PayConst.SXF_TYPE_RFDORDERQUERY;
            this.RefundTradeNo = req.RefundTradeNo;
            this.Version = PayConst.SXF_DEF_VERSION;
        }

        #region Properties
        /// <summary>
        /// 商户退款订单号，与请参数中数据一致，原样返回
        /// </summary>
        [JsonProperty("rfdOrderNo")]
        public string RefundTradeNo { get; set; }
        #endregion
    }
    /// <summary>
    /// 单笔退款订单查询响应参数
    /// </summary>
    [Serializable]
    public class SxfRefundQueryResponse : SxfBaseTradeResponse
    {
        #region Properties
        /// <summary>
        /// 商户退款订单号
        /// </summary>
        [JsonProperty("rfdOrderNo")]
        public string RefundTradeNo { get; set; }
        /// <summary>
        /// 退款金额
        /// </summary>
        [JsonProperty("rfdAmt")]
        public int RefundAmount { get; set; }
        /// <summary>
        /// 退款订单状态：F：失败； S：成功； U：预登记（退款中）
        /// </summary>
        [JsonProperty("rfdStatus")]
        public string RefundStatus { get; set; }
        /// <summary>
        /// 退款日期，格式yyyyMMdd
        /// </summary>
        [JsonProperty("rfdDate")]
        public string RefundDate { get; set; }
        /// <summary>
        /// 退款时间，格式 HHmmss
        /// </summary>
        [JsonProperty("rfdTime")]
        public string RefundTime { get; set; }
        #endregion

        #region Methods
        /// <summary>
        /// 
        /// </summary>
        /// <param name="merchStore"></param>
        /// <returns></returns>
        public RefundQueryResponse ToRefundQueryResponse(MerchantStore merchStore)
        {
            var obj = new RefundQueryResponse()
            {
                CID = merchStore.CID,
                DeviceId = string.IsNullOrEmpty(this.DeviceId) ? 0 : int.Parse(this.DeviceId),
                RefundTradeNo = this.RefundTradeNo,
                RefundDate = PayHelper.Convert2DateFormat(this.RefundDate, this.RefundTime, "yyyy-MM-dd HH:mm:ss"),
                RefundStatus = (PayHelper.Convert2EnumString<RefundStatus>(PayHelper.Convert2EnumValue<SxfRefundStatus>(this.RefundStatus))).ToUpper(),
                ReturnCode = this.ReturnCode,
                ReturnMsg = this.ReturnMsg,
                SID = merchStore.SID,
                SignType = this.SignType,
                RefundAmount = this.RefundAmount,
                Version = PayConst.DEF_VERSION
            };
            return obj;
        }
        #endregion
    }
    #endregion
}
