@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_ManagerLayout.cshtml";
    ViewData.OpBtnInfo(new OpBtnInfo(addText: "新增会员", delText: "移除", hideDel: true,searchHeight:70));
}
@section search{
    <table class="table-toolbar">
        <tr>
            <td class="label-toolbar">
                <label>来源：</label>
            </td>
            <td class="input-toolbar">
                <select class="easyui-combobox" data-options="width:120"><option>全部</option><option>门店1</option></select>
            </td>
            <td class="label-toolbar">
                <label>会员分组：</label>
            </td>
            <td class="input-toolbar">
                <select class="easyui-combobox" data-options="width:120"><option>全部</option><option>分组1</option></select>
            </td>
            <td class="label-toolbar">
                <label>居住地：</label>
            </td>
            <td class="input-toolbar">
                <select class="easyui-combobox" data-options="width:120"><option>全部</option><option>厦门市</option></select>
            </td>
            <td class="label-toolbar">
                <label>会员等级：</label>
            </td>
            <td class="input-toolbar">
                <select class="easyui-combobox" data-options="width:120"><option>全部</option><option>等级1</option></select>
            </td>
        </tr>
        <tr>
            <td class="label-toolbar">
                <label>建档日期:</label>
            </td>
            <td class="input-toolbar">
                <input name="date1" class="datacontrol Wdate" style="width:120px" />
            </td>
            <td style="float:left;margin-left:7px;margin-right:7px;line-height:28px;">-</td>
            <td class="input-toolbar">
                <input name="date2" class="datacontrol Wdate" style="width:120px" />
            </td>
            <td class="label-toolbar">
                <label>关键词：</label>
            </td>
            <td class="input-toolbar">
                <select class="easyui-combobox">
                    <option>会员编号</option>
                    <option>会员名称</option>
                    <option>卡面卡号</option>
                    <option>手机号</option>
                    <option>微信号</option>
                    <option>支付宝</option>
                    <option>Email</option>
                </select>
            </td>
            <td class="input-toolbar">
                &nbsp;<input id="title" name="title" class="datacontrol easyui-textbox" data-options="prompt:''" />
            </td>
        </tr>
    </table>
}
@section toolbar
{
    <a class="easyui-linkbutton cus1 linkbtn" data-options="iconCls:'icon-usable'" onclick="">审核通过</a>
    <a class="easyui-linkbutton cus1 linkbtn" data-options="iconCls:'icon-usable'" onclick="">设为无效</a>
    <a class="easyui-linkbutton cus1 linkbtn" data-options="iconCls:'icon-suspend'" onclick="openDialog600('会员分组','@Url.Action("CreateMemberGroup")')">会员分组</a>
}

<script type="text/javascript">
    pharos.manager.geturl = "@Url.Action("FindMemberPageList")";
    pharos.manager.editurl = "@Url.Action("CreateMember")";
    pharos.manager.columns = [[
        { field: 'Id', checkbox: true },
        { field: 'StateTitle', title: '来源', width: 80 },
        { field: 'StateTitle2', title: '编号', width: 80 },
        { field: 'StateTitle3', title: '名称', width: 80 },
        { field: 'StateTitle4', title: '等级', width: 80 },
        { field: 'StateTitle5', title: '性别', width: 80 },
        { field: 'StateTitle6', title: '可用积分', width: 80 },
        { field: 'StateTitle7', title: '可用余额', width: 80 },
        { field: 'StateTitle8', title: '累计充值', width: 80 },
        { field: 'StateTitle9', title: '累计消费', width: 80 },
        { field: 'StateTitle0', title: '默认优惠方式', width: 80 },
        { field: 'StateTitle10', title: '默认积分方式', width: 80 },
        { field: 'StateTitle11', title: '会员卡片', width: 80 },
        { field: 'StateTitle12', title: '优惠券', width: 80 },
        { field: 'StateTitle13', title: '证件号', width: 80 },
        { field: 'StateTitle14', title: '生日', width: 80 },
        { field: 'StateTitle15', title: '手机', width: 80 },
        { field: 'StateTitle16', title: '微信号', width: 80 },
        { field: 'StateTitle17', title: '支付宝', width: 80 },
        { field: 'StateTitle18', title: 'Email', width: 80 },
        { field: 'StateTitle19', title: '居住地址', width: 80 },
        { field: 'StateTitle20', title: '会员状态', width: 80 },
        { field: 'StateTitle21', title: '建档人员', width: 80 },
        { field: 'StateTitle22', title: '建档时间', width: 80 },
    ]];
</script>