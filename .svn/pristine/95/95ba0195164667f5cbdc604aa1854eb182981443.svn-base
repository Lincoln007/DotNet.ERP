using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Text.RegularExpressions;

namespace Pharos.Logic.ApiData.Pos.Common
{
    /// <summary>
    /// 流水号生成器
    /// </summary>
    public class PaySn
    {
        public string CurrentAutoIncrement { get; private set; }



        /// <summary>
        /// 创建新的流水号
        /// </summary>
        /// <returns></returns>
        public static string New()
        {
            var orderSN = DateTime.Now.ToString("yyyyMMddHHmmssfff");
            Random random = new Random();
            return MachinesSettings.OrderSnIdentifierChar + orderSN;
        }

    }
}
