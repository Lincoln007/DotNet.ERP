using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace Pharos.Logic.ApiData.Pos.Common
{
    /// <summary>
    /// 流水号生成器
    /// </summary>
    public class PaySn
    {
        /// <summary>
        /// 创建新的流水号
        /// </summary>
        /// <returns></returns>
        public static string New()
        {
            var orderSN = DateTime.Now.ToString("yyyyMMddHHmmssffff");
            Random random = new Random();
            var randomValue = random.Next(100, 999);
            return orderSN + randomValue.ToString();
        }
    }
}
