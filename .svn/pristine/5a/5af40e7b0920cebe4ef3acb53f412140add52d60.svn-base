html, body {
    font-family: 'Microsoft YaHei UI';
    font-size: 16px;
    padding: 0;
    margin: 0;
}

body {
    background: #f3f3f3;
}

ul {
    padding: 0;
    margin: 0;
}

li {
    list-style: none;
}

a, a:link {
    color: #fff;
    text-decoration: none;
    display: block;
}

header {
    background: #fff;
    height: 70px;
    border-bottom: 1px solid #e1e1e1;
}

    header a {
        float: right;
        margin-right: 3%;
        line-height: 70px;
        color: #515151;
        cursor:pointer;
    }

    header a:hover{
        color:#f24f2e;
    }

    .text-button{
        border: none; 
        background: none!important; 
        color: #515151!important; 
        font-size: 16px; 
        outline: none; 
        padding: 0; 
        font-weight: 400; 
        font-family: 'Microsoft YaHei UI';
        cursor:pointer;
    }

    .text-button:hover{
         color:#f24f2e!important;
    }

.menu {
    background: #0c1f2e;
    height: 100%;
    position: fixed;
    float: left;
    width: 14%;
}

    .menu li {
        color: #fff;
        margin: 0;
        padding: 16px 30px 16px 40px;
        list-style: none;
    }

    .menu div {
        padding-top: 23px;
        padding-bottom: 23px;
        text-align: center;
        font-size: 20px;
        color: #fff;
    }

    .menu li:hover, .menu .active {
        background: #234a6a;
        border-left: 5px solid #2196f3;
    }

.container {
    width: 84%;
    float: right;
    padding: 0 1%;
    position: relative;
    display: table;
}

    .container h4 {
        color: #515151;
        font-weight: 400;
    }

.item {
    width: 100%;
    height: auto;
    background: #fff;
    display: table;
}

.status li {
    width: 25%;
    float: left;
    height: 150px;
    list-style: none;
}

.status01 {
    width: 100%;
    height: 85px;
    background: url("../images/icon_status01.png") no-repeat center 95%;
    display: block;
}

.status02 {
    background: url("../images/icon_status02.png") no-repeat center 95%;
}

.status03 {
    background: url("../images/icon_status03.png") no-repeat center 95%;
}

.status04 {
    background: url("../images/icon_status04.png") no-repeat center 95%;
}

.status p {
    width: 100%;
    color: #515151;
    text-align: center;
}

.status-green{
    display: inline-block; 
    width: 30px; 
    height: 20px; 
    border-radius: 10px; 
    background: #5dd86c; 
    margin-left: 10px; 
    vertical-align: text-bottom;
}

.status-red{
    background:#f24f2e;
}

.status-gray{
    background:#c6c6c6;
}

.data {
    margin: 30px 40px;
    color: #515151;
    font-size:14px;
}

.data td{
    padding:5px 0 5px 25px;
    background:url("../images/icon_blog.png") no-repeat left center;
}

    .data span {
        margin-left: 10px;
        color: #2196f3;
    }

    .data .finish{
        color:#5dd86c;
    }

    .data .wrong{
        color:#f24f2e;
    }

h4 span {
    margin-left: 10px;
    color: #c6c6c6;
}

/*服务设置*/
.form-group {
    position: relative;
    font-size: 14px;
    color: #515151;
}

    .form-group + .form-group {
        margin-top: 30px;
    }

    .form-group .form-label {
        position: absolute;
        z-index: 1;
        left: 0;
        top: 5px;
        -webkit-transition: 0.3s;
        transition: 0.3s;
    }

    .form-group .form-control {
        width: 600px;
        position: relative;
        z-index: 3;
        height: 35px;
        line-height: 35px;
        background: none;
        border: none;
        padding: 5px 0;
        -webkit-transition: 0.3s;
        transition: 0.3s;
        border-bottom: 1px solid #e1e1e1;
    }

        .form-group .form-control:invalid {
            outline: none;
        }

        .form-group .form-control:focus, .form-group .form-control:valid {
            outline: none;
            color: #2196f3;
            box-shadow: 0 1px #2196f3;
            border-color: #2196f3;
        }

            .form-group .form-control:focus + .form-label, .form-group .form-control:valid + .form-label {
                font-size: 12px;
                -ms-transform: translateY(-15px);
                -webkit-transform: translateY(-15px);
                transform: translateY(-15px);
            }

.btn-item {
    clear: both;
    margin: 100px auto;
    display: table;
}

.btn {
    width: 200px;
    height: 50px;
    border-radius: 25px;
    background: url("../images/icon_arrow_right.png") no-repeat 92% center;
    background-color: #2196f3;
    border: none;
    text-align: center;
    line-height: 50px;
    font-size: 14px;
    color: #fff;
    cursor: pointer;
    outline: none;
    margin-top: 162px;
}

    .btn:hover {
        opacity: 0.8;
    }

    .btn:active {
        opacity: 0.9;
    }

.btn-little {
    width: 100px;
    height: 35px;
    border-radius: 17.5px;
    border: 1px solid #2196f3;
    text-align: center;
    line-height: 35px;
    color: #2196f3;
    cursor: pointer;
    outline: none;
    background: #fff;
}

    .btn-little:hover {
        background: #2196f3;
        color: #fff;
    }

    .btn-little:active {
        background: #2196f3;
        opacity: 0.9;
    }

.btn-gray {
    background: url("../images/icon_arrow_left.png") no-repeat 8% center;
    background-color: #d7d7d7;
    color: #7f7f7f;
}

.btn-save {
    background-image: none;
}

/*POS配置*/
.item-content {
    width: 90%;
    margin-top: 50px;
    margin-left:auto;
    margin-right:auto;
}

.item-content .row{
    border-bottom:1px dashed #e1e1e1;
    padding:30px 0;
}

.text-title {
    font-size: 16px;
    color: #515151;
    /*padding: 50px 0;*/
}

.text-content {
    font-size: 14px;
    color: #c6c6c6;
    padding: 50px 0;
}

.item-content select {
    appearance: none;
    -moz-appearance: none;
    -webkit-appearance: none;
    width: 150px;
    height: 35px;
    float: right;
    border-radius: 17.5px;
    border: 1px solid #d7d7d7;
    line-height: 35px;
    outline: none;
    padding-left: 10px;
    padding-right: 20px;
    background: url("../images/icon_arrow_down.png") no-repeat scroll 95% center transparent;

}

select::-ms-expand {
    display: none;
}

.item-content select:focus{
    background: url("../images/icon_arrow_up.png") no-repeat scroll 95% center transparent;

}

.item-content .option-new{
    border:1px solid #d7d7d7;
    outline:none;
    height:30px;
}
