<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:ChildTabContentControls="clr-namespace:Pharos.Barcode.Retailing.ChildTabContentControls" x:Class="Pharos.Barcode.Retailing.MainWindow"
        Title="条码价签打印" Height="600" Width="1024" Icon="favicon.ico">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles\BasePageStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <TabControl BorderThickness="1">
        <TabItem Header="商品列表">
            <ChildTabContentControls:BarcodePrintContentControl />
        </TabItem>
        <TabItem Header="服务器设置"><ChildTabContentControls:SystemManagement /></TabItem>
    </TabControl>
</Window>
