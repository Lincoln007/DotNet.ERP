@model IEnumerable<string>
@{
    var colcount = 3;
    var i = 0;
}
<table class="ListPartial">
    <tr>
        @foreach (var name in Model ?? new List<string>())
        {
            if (i == colcount)
            {
                ViewContext.Writer.Write("</tr><tr>");
                i = 0;
            }
            <td><label style="display:inline-block;">@name</label><img src="~/Content/image/close.png" width="12" height="12" onclick="delType('@name')" /></td>
            i++;
        }
    </tr>
</table>