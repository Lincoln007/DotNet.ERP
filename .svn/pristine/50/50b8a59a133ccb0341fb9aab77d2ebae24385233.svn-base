html,body {
    font-family:'Microsoft YaHei UI';
    font-size:16px;
    background:#f3f3f3;
    padding:0;
    margin:0;
}
ul{
    padding:0;
    margin:0;
}
a,a:link{
    color:#fff;
    text-decoration:none;
    display:block;
}
header{
    background:#fff;
    height:70px; 
    border-bottom:1px solid #e1e1e1;
}
header span{
    float:right;
    margin-right:3%;
    line-height:70px;
    color:#515151;
}
.menu{
    background:#0c1f2e;
    height:100%;
    position:fixed;
    float:left;
    width:14%;
}
.menu li{
    color:#fff;
    margin:0;
    padding:16px 30px 16px 40px;
    list-style:none;
}
.menu div{
    padding-top:23px;
    padding-bottom:23px;
    text-align:center;
    font-size:20px;
    color:#fff;
}
.menu li:hover,.menu .active{
    background:#234a6a;
    border-left:5px solid #2196f3;
}
.container{
    width:84%;
    float:right;
    padding:0 1%;
    position:relative;
    display:table;
}
.container h4{
    color:#515151;
    font-weight:400;
}
.item{
    width:100%;
    height:auto;
    background:#fff;
    display:table;
}
.status li{
    width:25%;
    float:left;
    height:150px;
    list-style:none;
}
.status01{
    width:100%;
    height:85px;
    background:url("../images/icon_status01.png") no-repeat center 95%;
    display:block;
}
.status02{
     background:url("../images/icon_status02.png") no-repeat center 95%;
}
.status03{
     background:url("../images/icon_status03.png") no-repeat center 95%;
}
.status04{
     background:url("../images/icon_status04.png") no-repeat center 95%;
}
.status p{
    width:100%;
    color:#515151;
    text-align:center;
}
.data{
    margin:30px 40px;
    color:#515151;
}
.data span{
    margin-left:10px;
    color:#2196f3;
}
h4 span{
    margin-left:10px;
    color:#c6c6c6;
}

/*服务设置*/
.form-group {
  position: relative;
  font-size: 14px;
  color: #515151;
}
.form-group + .form-group {
  margin-top: 30px;
}
.form-group .form-label {
  position: absolute;
  z-index: 1;
  left: 0;
  top: 5px;
  -webkit-transition: 0.3s;
  transition: 0.3s;
}
.form-group .form-control {
  width: 600px;
  position: relative;
  z-index: 3;
  height: 35px;
  line-height:35px;
  background: none;
  border: none;
  padding: 5px 0;
  -webkit-transition: 0.3s;
  transition: 0.3s;
  border-bottom: 1px solid #e1e1e1;
}
.form-group .form-control:invalid {
  outline: none;
}
.form-group .form-control:focus, .form-group .form-control:valid {
  outline: none;
  color: #2196f3;
  box-shadow: 0 1px #2196f3;
  border-color: #2196f3;
}
.form-group .form-control:focus + .form-label, .form-group .form-control:valid + .form-label {
  font-size: 12px;
  -ms-transform: translateY(-15px);
  -webkit-transform: translateY(-15px);
  transform: translateY(-15px);
}

.btn{
    width:200px;
    height:50px;
    border-radius:25px;
    background:url("../images/icon_arrow_right.png") no-repeat 92% center;
    background-color:#2196f3;
    border:none;
    text-align:center;
    line-height:50px;
    font-size:14px;
    color:#fff;
    cursor:pointer;
    outline:none;
    margin-top:162px;
}
.btn:hover{
    opacity:0.8;
}
.btn:active{
    opacity:0.9;
}