@model Pharos.Logic.OMS.Entity.ProductRole
@{
    Layout = "~/Views/Shared/_FormLayout.cshtml";
}

<div class="default-form">
    @using (Html.BeginForm())
    {
        @Html.HiddenFor(o => o.Id)
        @Html.HiddenFor(o => o.RoleVerId)
        <div class="content">
            <table class="table-form" width="100%" height="160px">
                <tr>
                    <td class="name">角色编号：</td>
                    <td class="input">@Html.TextBoxFor(o => o.RoleId, new { @class = "easyui-textbox",data_options = "readonly:true,prompt:'自动生成...'" })</td>
                </tr>
                <tr>
                    <td class="name">角色名称：</td>
                    <td class="input">@Html.TextBoxFor(o => o.Title, new { @class = "easyui-textbox", data_options = "required:true,validType:'length[1,50]'" })</td>
                </tr>
            </table>
        </div>
        
    }
</div>
<script type="text/javascript">
    function SaveBefore() {
        return true;
    }
</script>