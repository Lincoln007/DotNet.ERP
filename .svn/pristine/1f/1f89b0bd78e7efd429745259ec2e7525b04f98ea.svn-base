using Pharos.POS.Retailing.ChildWin;
using Pharos.Wpf.HotKeyHelper;
using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Windows;
using System.Windows.Input;

namespace Pharos.POS.Retailing.ObjectModels.HotKeyCommand
{
    public class ProductEditorCommand : IHotKeyCommand
    {
        public Action<Window> Handler
        {
            get
            {
                return new Action<Window>((win) =>
                {
                    ProductEditor page = new ProductEditor();
                    page.Owner = win;
                    page.ShowDialog();
                });
            }
        }
    }
}
