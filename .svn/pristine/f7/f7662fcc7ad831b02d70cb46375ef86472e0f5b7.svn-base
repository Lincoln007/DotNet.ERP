using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace Pharos.Logic.Entity
{
    public abstract class BaseSupplier
    {
        /// <summary>
        /// 供应商ID
        /// [主键：√]
        /// [长度：40]
        /// [不允许为空]
        /// </summary>
        public string Id { get; set; }

        /// <summary>
        /// 供应商分类（-1:未知）（来自数据字典表）
        /// [长度：10]
        /// [不允许为空]
        /// [默认值：((-1))]
        /// </summary>
        public int ClassifyId { get; set; }

        /// <summary>
        /// 商家简称
        /// [长度：50]
        /// [不允许为空]
        /// </summary>
        public string Title { get; set; }

        /// <summary>
        /// 商家简拼
        /// [长度：20]
        /// </summary>
        public string Jianpin { get; set; }

        /// <summary>
        /// 商家全称
        /// [长度：100]
        /// [不允许为空]
        /// </summary>
        public string FullTitle { get; set; }

        /// <summary>
        /// 联系人
        /// [长度：20]
        /// [不允许为空]
        /// </summary>
        public string Linkman { get; set; }

        /// <summary>
        /// 手机
        /// [长度：11]
        /// </summary>
        public string MobilePhone { get; set; }

        /// <summary>
        /// 电话
        /// [长度：20]
        /// </summary>
        public string Tel { get; set; }

        /// <summary>
        /// E-mail
        /// [长度：100]
        /// </summary>
        public string Email { get; set; }

        /// <summary>
        /// 地址
        /// [长度：100]
        /// </summary>
        public string Address { get; set; }

        /// <summary>
        /// 指派人（UID）
        /// [长度：40]
        /// [不允许为空]
        /// [默认值：((-1))]
        /// </summary>
        public string Designee { get; set; }

        /// <summary>
        /// 主账号
        /// [长度：100]
        /// </summary>
        public string MasterAccount { get; set; }

        /// <summary>
        /// 密码
        /// [长度：50]
        /// </summary>
        public string MasterPwd { get; set; }

        /// <summary>
        /// 账号状态（0:关闭、1:可用）
        /// [长度：5]
        /// [不允许为空]
        /// [默认值：((0))]
        /// </summary>
        public short MasterState { get; set; }

        public DateTime? CreateDT { get; set; }

        /// <summary>
        /// 商家类型（1：供应商、2：批发商）
        /// </summary>
        public short BusinessType { get; set; }
        /// <summary>
        /// 手势密码
        /// </summary>
        public string Handsign { get; set; }
    }
}
