@model  OMS_CompanyAuthorize
@{
    ViewBag.Title = "Save";
    Layout = "~/Views/Shared/_FormLayout.cshtml";
}
<div class="default-form">
    @using (Html.BeginForm())
    {

        <div class="content">
            <table class="table-form" width="100%" height="200px">
                <tr>
                    <td class="name">分类：</td>
                    <td class="input">@Html.DropDownListFor(o => o.AppProper, new List<SelectListItem>() { new SelectListItem() { Text = "请选择", Value = "" }, new SelectListItem() { Text = "mPOS一体机", Value = "1" }, new SelectListItem() { Text = "mPOS刷卡器", Value = "2" } }, new { @class = "easyui-combobox datacontrol", data_options = "required:true,novalidate:true,validType:'requiredForCombo', editable: false" })</td>
                    <td class="name">名称：</td>
                    <td class="input">@Html.TextBoxFor(o => o.Title, new { @class = "easyui-textbox", data_options = "novalidate:true,required:true,prompt:'名称'" })</td>
                </tr>
                <tr>
                    <td class="name">品牌：</td>
                    <td class="input">@Html.TextBoxFor(o => o.FullTitle, new { @class = "easyui-textbox", data_options = "novalidate:true,required:true,prompt:'品牌'" })</td>
                    <td class="name">型号：</td>
                    <td class="input">@Html.TextBoxFor(o => o.Title, new { @class = "easyui-textbox", data_options = "novalidate:true,required:true,prompt:'型号'" })</td>
                </tr>
                <tr>
                    <td class="name">状态：</td>
                    <td class="input">@Html.RadioButtonListFor(o => o.Category, new List<SelectListItem>() { new SelectListItem() { Text = "可用", Value = "1" }, new SelectListItem() { Text = "停用", Value = "2" } })</td>
                </tr>
            </table>
        </div>
    }
</div>
