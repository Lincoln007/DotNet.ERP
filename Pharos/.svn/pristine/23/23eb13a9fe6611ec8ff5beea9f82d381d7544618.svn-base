        public string TPaySecrectId
        {
            get { return _TPaySecrectId; }
            set { _TPaySecrectId = value; }
        }
        private string _TPaySecrectId;

        /// <summary>
        /// 通道状态（0:未审核，1:可用，2:暂停，3:注销，4:无效）
