<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="ErrorMessageStyle.xaml"></ResourceDictionary>
        <ResourceDictionary Source="BaseButtonStyle.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <Style BasedOn="{StaticResource BtnBase}" TargetType="Button" x:Key="BtnOkStyle">
        <Setter Property="FontSize" Value="20"></Setter>
        <Setter Property="Margin" Value="5"></Setter>
        <Setter Property="Foreground" Value="#FEFDFF"></Setter>
        <Setter Property="BorderThickness" Value="0"></Setter>
        <Setter Property="Height" Value="60"></Setter>
        <Setter Property="Width" Value="200"></Setter>
        <Setter Property="Background" Value="#659D15"></Setter>
    </Style>
    <Style TargetType="TextBlock" x:Key="txtBase">
        <Setter Property="FontSize" Value="16"></Setter>
        <Setter Property="Foreground" Value="#404040"></Setter>
    </Style>
    <Style  BasedOn="{StaticResource baseTxtErrorTPL}" x:Key="txtJine"  TargetType="TextBox">
        <Setter Property="Width" Value="170"></Setter>
        <Setter Property="Height" Value="40"></Setter>
        <Setter Property="TextAlignment" Value="Center"></Setter>
        <Setter Property="VerticalAlignment" Value="Center"></Setter>
        <Setter Property="VerticalContentAlignment" Value="Center"></Setter>
        <Setter Property="FontSize" Value="16"></Setter>
    </Style>
    <Style   TargetType="PasswordBox">
        <Setter Property="Width" Value="170"></Setter>
        <Setter Property="Height" Value="40"></Setter>
        <Setter Property="VerticalAlignment" Value="Center"></Setter>
        <Setter Property="VerticalContentAlignment" Value="Center"></Setter>
        <Setter Property="FontSize" Value="20"></Setter>
    </Style>
</ResourceDictionary>