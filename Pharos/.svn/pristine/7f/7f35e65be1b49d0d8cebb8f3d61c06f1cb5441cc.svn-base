using Pharos.POS.Retailing.ChildWin;
using Pharos.Wpf.HotKeyHelper;
using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Windows;

namespace Pharos.POS.Retailing.ObjectModels.HotKeyCommand
{
   public class ChaJiaGeCommand : IHotKeyCommand
   {
       public Action<Window> Handler
       {
           get
           {
               return new Action<Window>((win) =>
               {
                   ChaJiaGe page = new ChaJiaGe();
                   page.Owner = win;
                   page.ShowDialog();
               });
           }
       }
    }
}
