@{
    ViewBag.Title = "CreateRecharge";
    Layout = "~/Views/Shared/_FormLayout.cshtml";
}

<div class="default-form">
    @using (Html.BeginForm())
    {
        <div class="content">
            <table class="table-form" width="100%">
                <tr>
                    <td class="name">新增数量：</td>
                    <td class="input"><input type="text" class="easyui-textbox" /></td>
                    <td class="name">类型：</td>
                    <td class="input"><select class="easyui-combobox"><option>请选择</option></select></td>
                </tr>
                <tr>
                    <td class="name">编号范围：</td>
                    <td class="input"><input type="text" class="easyui-textbox" data-options="width:70" />-<input type="text" class="easyui-textbox" data-options="width:70" /></td>
                    <td class="name">有效期限：</td>
                    <td class="input"><input type="text" class="easyui-validatebox Wdate" /></td>
                </tr>
            </table>
        </div>
    }
</div>