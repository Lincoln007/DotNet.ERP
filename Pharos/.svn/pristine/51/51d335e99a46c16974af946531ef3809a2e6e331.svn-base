/*登录页*/
#login .head { position: relative; height: 70px; background: #2d3339 url('../../../SysImg/logo.png') no-repeat; }
#login .content { width: 380px; height: 473px; position: relative; margin-top: 30px; margin-left: 50%; left: -190px; background: url('images/login/login.png') no-repeat; z-index: 100; }
#login #userName { position: absolute; top: 131px; left: 92px; height: 16px; padding: 18px 10px; width: 233px; border: 0; outline: none; }
#login #userPwd { position: absolute; top: 226px; left: 92px; height: 16px; padding: 18px 10px; width: 233px; border: 0; outline: none; }
#login #remember { position: absolute; top: 310px; left: 30px; color: white; font-size: 1.2em; }
#login #remember input { vertical-align: middle; cursor: pointer; }
#login #submit { position: absolute; display: block; cursor: pointer; width: 315px; height: 53px; top: 358px; left: 33px; }
#login #submit:hover { background-color: white; filter: alpha(opacity=10); -moz-opacity: 0.1; -khtml-opacity: 0.1; opacity: 0.1; }
#login .msg { text-align: center; color: #e2315d; }
#login p { padding: 1em 0; }
#login #userName, #login #userPwd { _width: 233px; _height: 52px; _padding: 0 10px; _line-height: 52px; }

/*首页*/
.header { background: #2d3339; position: relative; height: 70px; color: #dfdfdf;  }
.header a { color: #dfdfdf; margin-left: 10px; }
.header a:last-child { margin-right: 20px; }
.header i { margin-left: 10px; font-style: normal; }
.footer { text-align: center; line-height: 32px; position:relative;height:32px;overflow:hidden;}
.footer img { vertical-align: middle; margin-right: 10px; }

/*菜单*/
ul#menu{padding:5px 0;}
.tree-node{padding:2px 0;}
.rowactive{background-color:#daeef5;}
#toolbar{padding:10px;}

/*首页菜单*/

/*表单*/
.default-form{padding:0px;margin:0 auto;}
.default-form h1{font-size:1.2em;font-weight:normal;padding-bottom:1em;}
.default-form .content{/*border: 1px solid #d9d9d9;*/padding:10px 10px;*overflow:auto;margin-bottom:5px;-webkit-border-radius: 5px; -moz-border-radius: 5px; -o-border-radius: 5px; -ms-border-radius: 5px; border-radius: 5px;}
.default-form .content .clearfix{padding-bottom:10px;*overflow:auto;}
.default-form .content .clearfix .label{width:80px;height:30px;line-height:30px;float:left;}
.default-form .content .clearfix label.error{color:red; float:right; width:200px;}
.default-form .content .clearfix div{float:left;}
.default-form .content .input-radio{margin:10px 0;vertical-align:middle;*margin:5px 0;}
.default-form .content .input-text{height:16px;line-height:16px;padding:6px 10px;border: 1px solid #d9d9d9;width:140px;_height:28px;_width:140px;vertical-align:middle;-webkit-border-radius: 5px; -moz-border-radius: 5px; -o-border-radius: 5px; -ms-border-radius: 5px; border-radius: 5px;}
.default-form .content select{ background:0;border:0;height:28px;line-height:28px;border: 1px solid #d9d9d9;width:160px;vertical-align:middle; padding-left:10px;}
.default-form .content .msg{margin-left:10px;color:red;display:inline-block;}
.datacontrol { border: 1px solid #D3D3D3; border-radius:5px;height: 26px; vertical-align: middle; outline-style: none; width:150px;padding-left:7px;padding-right:7px;line-height:26px;}
.combo,.combo-arrow,.textbox{height:26px!important;}
.combo a{margin-right:0px!important; }
/*.combo .textbox-text{height:92%;}*/
.window-body{overflow:hidden!important;}
form label.error {color:red;overflow:hidden;}
form .table {
    width:100%; table-layout:fixed;
    word-break:break-all; word-wrap:break-word;
}
.default-form .name{text-align:right;}
.table-form .easyui-validatebox,.table-form .easyui-textbox,.table-form .easyui-numberbox,.table-form .easyui-datebox,.Wdate
  {border: 1px solid #D3D3D3; border-radius:5px;height: 26px; vertical-align: middle; outline-style: none; width:150px;padding-left:7px;padding-right:7px;line-height:26px;}
.table-form .easyui-datebox {width:145px;}
.table-form .validatebox-invalid { border-color: #ffa8a8; background-color: #fff3f3; color: #000; }
.input-text{height:16px;line-height:16px;padding:6px 10px;border: 1px solid #d9d9d9;width:200px;_height:28px;_width:220px;vertical-align:middle;-webkit-border-radius: 5px; -moz-border-radius: 5px; -o-border-radius: 5px; -ms-border-radius: 5px; border-radius: 5px;}
.input-radio{margin:10px 0;vertical-align:middle;*margin:5px 0;}
select{ border:0;height:28px;line-height:28px;border: 1px solid #d9d9d9;vertical-align:middle;}

/*查询区域*/
.table-toolbar { min-width:200px;min-height:38px; }
.label-toolbar { line-height: 29px; text-align: right; float: left; margin:0px 6px 0px 10px;}
.input-toolbar { float:left; text-align:left; min-width:80px;}
.input-toolbar-1{margin-left:-10px;}
.search-toolbar { float:left; margin-left:25px; }
.height-single { height:38px; overflow:auto; } /*查询区域占一行的高度*/
.height-double { height:72px; overflow:auto; } /*查询区域占两行的高度*/
.button-add { margin-left:24px; } /*第二行的添加按钮*/
.inputlabel{background-color:transparent;border:0px;}
.formbox{border-bottom: 1px dashed #999999;padding-bottom:10px;}
input::-ms-clear{display : none; }/*禁用删除小标*/