<control:DialogWindow02  x:Class="Pharos.POS.Retailing.ChildWin.Goto"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:control="clr-namespace:Pharos.Wpf.Controls;assembly=Pharos.Wpf"
        Title="定位" Height="200" Width="270" WindowStartupLocation="CenterScreen" Icon="..\Images\ChaKuCun\search.png" Margin="0" WindowStyle="None">
    <Window.Resources>
        <Style x:Key="titleStyle" TargetType="TextBlock">
            <Setter Property="VerticalAlignment" Value="Center"></Setter>
            <Setter Property="HorizontalAlignment" Value="Right"></Setter>
            <Setter Property="FontSize" Value="16"></Setter>
        </Style>
    </Window.Resources>
    <Grid Height="50">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="50"></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition Width="Auto"></ColumnDefinition>
            <ColumnDefinition Width="60"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <TextBlock Text="定位到" Style="{StaticResource titleStyle}"></TextBlock>
        <control:IconTextBox Name="txtGoto" Text="{Binding Index,Mode=OneWayToSource}" Height="35" Grid.Column="1" FontSize="16" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"></control:IconTextBox>
        <TextBlock Text="行" Grid.Column="2" Style="{StaticResource titleStyle}"></TextBlock>

        <control:PosButton Command="{Binding Command}" Grid.Column="3" Content="&#xf0a9;" FontFamily="/Pharos.Wpf;component/Fonts/#FontAwesome" FontSize="35">
        </control:PosButton>
    </Grid>
</control:DialogWindow02>
