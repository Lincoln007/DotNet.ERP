<control:DialogWindow02 x:Class="Pharos.POS.Retailing.ChildWin.ChuRuKuan"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:control="clr-namespace:Pharos.Wpf.Controls;assembly=Pharos.Wpf"
        Title="出入款" Height="400" Width="700" WindowStartupLocation="CenterScreen" Icon="..\Images\ChaKuCun\search.png" Margin="0" WindowStyle="None">
    <control:DialogWindow02.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="..\Styles\ChuRuKuanStyle.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </control:DialogWindow02.Resources>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="50"></RowDefinition>
            <RowDefinition Height="50"></RowDefinition>
            <RowDefinition Height="50"></RowDefinition>
            <RowDefinition Height="50"></RowDefinition>
            <RowDefinition Height="100"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"></ColumnDefinition>
            <ColumnDefinition Width="150"></ColumnDefinition>
            <ColumnDefinition Width="150"></ColumnDefinition>
            <ColumnDefinition Width="150"></ColumnDefinition>
            <ColumnDefinition Width="150"></ColumnDefinition>
            <ColumnDefinition Width="*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <TextBlock Grid.Column="1" Grid.Row="1" Style="{StaticResource titleStyle}" Text="POS机号："></TextBlock>
        <TextBlock Grid.Column="2" Grid.Row="1" Text="{Binding MachineSN}" Style="{StaticResource txtcontentStyle}"></TextBlock>
        <TextBlock Grid.Column="1" Grid.Row="2" Style="{StaticResource titleStyle}" Text="单位："></TextBlock>
        <TextBlock Grid.Column="2" Grid.Row="2" Text="元" Style="{StaticResource txtcontentStyle}"></TextBlock>
        <TextBlock Grid.Column="1" Grid.Row="3" Style="{StaticResource titleStyle}" Text="金额："></TextBlock>
        <control:IconTextBox Grid.Column="2" Grid.Row="3" Style="{StaticResource contentStyle}" Text="{Binding Amount,Mode=TwoWay}"></control:IconTextBox>
        <TextBlock Grid.Column="1" Grid.Row="4" Style="{StaticResource titleStyle}" Text="密码："></TextBlock>
        <control:IconPasswordBox Grid.Column="2" Grid.Row="4" Style="{StaticResource IconPasswordBoxStyle}" Password="{Binding Passsword,Mode=TwoWay}" ></control:IconPasswordBox>
        
        <TextBlock Grid.Column="3" Grid.Row="1" Style="{StaticResource titleStyle}" Text="收银员："></TextBlock>
        <TextBlock Grid.Column="4" Grid.Row="1" Style="{StaticResource txtcontentStyle}" Text="{Binding UserName}" ></TextBlock>

        <TextBlock Grid.Column="3" Grid.Row="2" Style="{StaticResource titleStyle}" Text="币种："></TextBlock>
        <TextBlock Grid.Column="4" Grid.Row="2" Style="{StaticResource txtcontentStyle}" Text="人民币"></TextBlock>
        
        <TextBlock Grid.Column="3" Grid.Row="3" Style="{StaticResource titleStyle}" Text="日期："></TextBlock>
        <TextBlock Grid.Column="4" Grid.Row="3" Style="{StaticResource txtcontentStyle}" Text="{Binding Time,Mode=TwoWay}"></TextBlock>
        <control:PosButton Style="{StaticResource BtnStyle}" IsEnabled="True" Grid.Row="5"  Grid.Column="2"  Command="{Binding OparetCommand}" CommandParameter="1">入款</control:PosButton>
        <control:PosButton Style="{StaticResource BtnStyle}" IsEnabled="True" Grid.Row="5"  Grid.Column="3" Background="#BCB4BB"  Command="{Binding OparetCommand}" CommandParameter="0">出款</control:PosButton>

    </Grid>
</control:DialogWindow02>
