@charset "utf-8";
/******= Reset =******/
section,article,aside,header,footer,nav,dialog,figure,legend{display:block;}
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,textarea,p,blockquote,th,td,figure{margin:0;padding:0;}
table{border-collapse:collapse;border-spacing:0;empty-cells:show; table-layout:fixed;}
table td{word-break:break-all;}
caption,th{text-align:left;}
ol,ul{list-style:none;}
img{display:block;border:0;vertical-align:middle;max-width:100%;height:auto;width:auto\9;/* ie8 */}
*:focus{outline:0;}
*{-webkit-tap-highlight-color:rgba(0,0,0,0);-webkit-touch-callout:none;-webkit-box-sizing:border-box;box-sizing:border-box}
audio,canvas,video{display:inline-block;*display:inline;*zoom:1;}
audio:not([controls]){display:none;}[hidden]{display:none;}
input[type=button]{-webkit-appearance:none;outline:none}
::-webkit-input-placeholder{color:#acacac;}
:-moz-placeholder{color:#acacac;}
::-moz-placeholder{color:#acacac;}

/******= Typography =******/
html,body{height:100%;}
html{font-size:20px;outline:0;font-family:Arial,Helvetica,sans-serif;}
body{color:#393939;background:#f7f7f7;-webkit-user-select:none;-webkit-text-size-adjust:none;font-family:'Microsoft YaHei';}
h1,h2,h3,h4,h5,h6{font-size:100%;font-weight:normal;}

/******= Common style =******/
.main{position:relative;min-width:320px;max-width:640px;min-height:100%;margin:0 auto;overflow:hidden;padding-top:0.95rem;}




/*新增客户*/
header{
    width:100%;
    padding:0.26rem 0; 
    text-align:center;
    font-size:0.3rem;
    color:#393939;
    background:#fff;
    border-bottom:1px solid #e1e1e1;
    position: fixed;
    min-width: 320px;
    max-width: 640px;
    overflow:hidden;
    z-index:999;
}

.icon_arrow_left{
    background:url("../images/icon_arrow_left.png") center no-repeat;
    background-size:0.33rem;
    width:0.33rem;
    height:0.33rem;
    display:block;
    position:absolute;
    margin-left:0.2rem;
}

.item{
    margin-bottom:-0.26rem;
}

.info_item{
    margin-top:0.2rem;
}

.info_row{
    width:100%;
    background:#fff;
    display:table;
    border-bottom:1px solid #e1e1e1;
    padding:0.26rem 0.2rem;

}

.info_tittle{
    font-size:0.26rem;
    float:left;
    width:26%;
}

.info_row input{
    float:right;
    height:0.4rem;
    width:67%;
    border:none;
    color:#797979;
    font-size:0.26rem;
    font-family:'Microsoft YaHei';
}

.select_item{
    width:67%;
    overflow:hidden;
    float:right;
   
}
.select_multitem{
    width:67%;
    float:right;
}
.select_multitem button{
    border:0px;
    
}
.select_multitem button:hover{
    background-color:#fff!important;
}
.select_multitem .filter-option{
    left:inherit!important;
    right:0px!important;
    font-size:0.26rem;
    font-family:'Microsoft YaHei';
    color:#797979;
}
.bootstrap-select{width:90%!important;}
.dropdown-menu{
    font-size:0.26rem!important;
    font-family:'Microsoft YaHei'!important;
}
.caret2{
    height:0.4rem;
    border:none;
    color:#797979;
    font-size:0.26rem;
    background:url("../images/icon_arrow_right.png") 99% center no-repeat;
    background-size:0.14rem;
    padding-right:0.3rem;
}
select {
    float: right;
    height: 0.4rem;
    width: 108%;
    border: none;
    color: #797979;
    font-size: 0.26rem;
    font-family: 'Microsoft YaHei';
    text-align: right !important;
    background: url("../images/icon_arrow_right.png") right center no-repeat;
    background-size: 0.14rem;
    direction: rtl;
    padding-right: 0.3rem;
}

option{
    direction:ltr;
}

.item_title{
    width:100%;
    padding:0.2rem;
    background:#f7f7f7;
    font-size:0.26rem;
    color:#797979;
}

.info_text{
    width:100%;
    padding:0.26rem 0.2rem;
    font-size:0.26rem;
    color:#797979;
    min-height:1rem;
    border:none;
    border-bottom:1px solid #e1e1e1;
    font-family:'Microsoft YaHei';
}

footer{
    width:100%;
    /*height:1rem;*/
    padding:0.2rem;
    background:#fff;
    border-top:1px solid #e1e1e1;
    position:fixed;
    bottom:0;
    font-size:0.26rem;
    z-index:999;
}

footer .button{
    width:100%;
    padding:0.3rem 0;
    background:#0fb7e6;
    border-radius:10px;
    color:#fff;
    text-align:center;
    border:none;
    font-size:0.26rem;
    display:inline-block;
}

footer .button:active{
    opacity:0.8;
}

.info_text1{
    width:100%;
    padding:0.1rem 0.2rem;
    color:#c6c6c6;
    font-size:0.2rem;
    text-align:right;
    margin:-0.1rem 0 2rem 0;
}

.info_num{
    color:#0fb7e6;
}

.icon_delete{
    width:0.3rem;
    height:0.3rem;
    background:url("../images/icon_delete.png") center no-repeat;
    background-size:0.3rem;
    display:block;
    float:right;
}

.info_row1{
    color:#393939;
    font-size:0.26rem;
    text-align:center;
    border:none;
    font-family:'Microsoft YaHei';
    margin:0.2rem 0 2rem 0;
}

.icon_add{
    width:0.33rem;
    height:0.33rem;
    background:url("../images/icon_add.png") left center no-repeat;
    background-size:0.33rem;
    padding-left:0.5rem;
}

#are{
    text-align:right;
    background:url("../images/icon_arrow_right.png") 99% center no-repeat;
    background-size:0.14rem;
    padding-right:0.3rem;
}

.red{
    color:#de5356;
}