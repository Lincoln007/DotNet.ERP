@{
    ViewBag.Title = "TakeCouponIndex";
    Layout = "~/Views/Shared/_ManagerLayout.cshtml";
    ViewData.OpBtnInfo(new OpBtnInfo(delText: "移除", hideDel: true,hideAdd:true,hideEdit:true));
}
@section search{
    <table class="table-toolbar">
        <tr>
            <td class="label-toolbar">
                <label>类别：</label>
            </td>
            <td class="input-toolbar">
                <select class="easyui-combobox"><option>全部</option></select>
            </td>
            <td class="label-toolbar">
                <label>形式：</label>
            </td>
            <td class="input-toolbar">
                <select class="easyui-combobox"><option>全部</option></select>
            </td>
            <td class="label-toolbar">
                <label>来源：</label>
            </td>
            <td class="input-toolbar">
                <select class="easyui-combobox"><option>全部</option></select>
            </td>
            <td class="label-toolbar">
                <label>使用期限：</label>
            </td>
            <td class="input-toolbar">
                <select class="easyui-combobox"><option>全部</option>
                    <option>10天</option>
                    <option>15天</option>
                    <option>1个月</option>
                    <option>2个月</option>
                    <option>3个月</option>
                    <option>4/5/6个月</option>
                </select>
            </td>
            <td class="label-toolbar">
                <label>派发员：</label>
            </td>
            <td class="input-toolbar">
                <select class="easyui-combobox"><option>全部</option></select>
            </td>
        </tr>
    </table>
}
@section toolbar
{
    <a class="easyui-linkbutton cus1 linkbtn" data-options="iconCls:'icon-usable'" onclick="">派发申领</a>
    <a class="easyui-linkbutton cus1 linkbtn" data-options="iconCls:'icon-usable'" onclick="">设为作废</a>
}

<script type="text/javascript">
    pharos.manager.geturl = "@Url.Action("FindTakeCouponPageList")";
    pharos.manager.columns = [[
        { field: 'Id', checkbox: true },
        { field: 'StateTitle', title: '券号', width: 80 },
        { field: 'StateTitle2', title: '类别', width: 80 },
        { field: 'StateTitle3', title: '形式', width: 80 },
        { field: 'StateTitle4', title: '来源', width: 80 },
        { field: 'StateTitle5', title: '适用商品', width: 80 },
        { field: 'StateTitle6', title: '状态', width: 80 },
        { field: 'StateTitle7', title: '领用时间', width: 80 },
        { field: 'StateTitle8', title: '领用人', width: 80 },
        { field: 'StateTitle9', title: '有效期限', width: 80 },
        { field: 'StateTitle10', title: '派发人', width: 80 },
    ]];

</script>