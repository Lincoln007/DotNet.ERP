
namespace Pharos.POS.Retailing.Models.Payment
{
    public class PayDetails
    {
        /// <summary>
        /// 备注
        /// </summary>
        public string Remark { get; set; }
        /// <summary>
        /// 需支付钱数
        /// </summary>
        public decimal MoneyNeedPay { get; set; }
        /// <summary>
        /// 单位
        /// </summary>
        public string Unit { get; set; }
        /// <summary>
        /// 支付流水号
        /// </summary>
        public string PaySN { get; set; }
        /// <summary>
        /// 是否为换货处理
        /// </summary>
        public bool IsReturnProduct { get; set; }

        public decimal BillDiscount { get; set; }

        /// <summary>
        /// 储值卡或者银行卡号
        /// </summary>
        public string CardNo { get; set; }

        public decimal Receipt { get; set; }

        /// <summary>
        /// 找零
        /// </summary>
        public decimal Change { get; set; }

        /// <summary>
        /// 收银员
        /// </summary>
        public string SaleManUid{ get; set; }
        public decimal Preferential { get; set; }


    }
}
