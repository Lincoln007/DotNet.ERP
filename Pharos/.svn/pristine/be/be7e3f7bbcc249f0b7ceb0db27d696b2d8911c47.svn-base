@model GenerateCoupon
@{
    ViewBag.Title = "TakeCoupon";
    Layout = "~/Views/Shared/_FormLayout.cshtml";
}

<div class="default-form">
    @using (Html.BeginForm())
    {
        <div class="content">
            <table class="table-form" width="100%" height="117px">
                <tr>
                    <td class="name">批次：</td>
                    <td class="input">
                        @Html.TextBoxFor(o => o.BatchSN, new { @class = "easyui-validatebox", data_options = "required:true" })
                    </td>
                </tr>
                <tr>
                    <td class="name">券号：</td>
                    <td class="input">
                        @Html.TextBoxFor(o => o.TicketNo, new { @class = "easyui-validatebox", data_options = "required:true" })
                    </td>
                    <td>
                        <label style="text-align:center;">-</label>
                    </td>
                    <td class="input">
                        @Html.TextBoxFor(o => o.TicketNo, new { @class = "easyui-validatebox", data_options = "required:true" })
                    </td>
                </tr>
                <tr></tr>
                <tr></tr>
            </table>
        </div>
    }
</div>

