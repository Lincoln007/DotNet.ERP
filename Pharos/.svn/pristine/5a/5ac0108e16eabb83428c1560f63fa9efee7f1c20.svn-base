using Microsoft.Synchronization.Data.Server;
using System;
using System.Collections.Generic;
using System.Data.SqlClient;
using System.Linq;
using System.Text;

namespace Pharos.SyncWcfService
{
    public class PosDbServerSyncProvider : DbServerSyncProvider
    {
        public PosDbServerSyncProvider()
        {
            SqlConnection serverConn = new SqlConnection(System.Configuration.ConfigurationManager.ConnectionStrings["PosDBServer"].ConnectionString);
            this.Connection = serverConn; 
        }
    }
}
