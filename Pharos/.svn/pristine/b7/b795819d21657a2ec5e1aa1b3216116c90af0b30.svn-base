		/// <summary>
		/// 记录ID
		/// [主键：√]
		/// [长度：10]
		/// [不允许为空]
		/// </summary>
		public int Id
		{
			get { return _Id; }
			set { _Id = value; }
		}
		private int _Id;

        /// <summary>
        /// 代理商编号
        /// </summary>
        public int AgentsId { get; set; }

		/// <summary>
		/// 所属体系（1:云平台，2:外部）
		/// [长度：10]
		/// [不允许为空]
		/// </summary>
		public int SourceType
		{
			get { return _SourceType; }
			set { _SourceType = value; }
		}
		private int _SourceType;

		/// <summary>
		/// 商户号
		/// [长度：10]
		/// [不允许为空]
		/// </summary>
		public int CID
		{
			get { return _CID; }
			set { _CID = value; }
		}
		private int _CID;

        /// <summary>
        /// 商户安全密钥
        /// </summary>
        public string SecretKey { get; set; }

