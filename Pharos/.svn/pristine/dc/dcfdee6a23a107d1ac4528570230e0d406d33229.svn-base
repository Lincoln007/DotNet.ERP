@{
    ViewBag.Title = Pharos.Sys.SysConstPool.Page_Title;
    Layout = "~/Views/Shared/_FormLayout.cshtml";    
}

<style>
    #returnDetail tr td {
        width:150px;
    }
</style>

<table class="" id="returnDetail" style="line-height: 35px; margin-top:10px; min-width:680px; border-color: #797979; overflow-x:auto; ">
     <tr>
        <td class="left_td">货号：</td>
        <td>@Html.DisplayText("ProductCode")</td>
        <td class="left_td">品名：</td>
        <td>@Html.DisplayText("Title")</td>
    </tr>
    <tr>
        <td class="left_td">订货数量：</td>
        <td>@Html.DisplayText("IndentNum")@Html.DisplayText("SubUnit")</td>
        <td class="left_td">已配送：</td>
        <td>@Html.DisplayText("DeliveryNum")@Html.DisplayText("SubUnit")</td>
    </tr>
    <tr>
        <td class="left_td">订货日期：</td>
        <td>@Html.DisplayText("CreateDT")</td>
        <td class="left_td">实际收货数量：</td>
        <td>@Html.DisplayText("AcceptNum")@Html.DisplayText("SubUnit")</td>
    </tr>
    <tr>
        <td class="left_td">收货日期：</td>
        <td>@Html.DisplayText("DeliveryDate")</td>
        <td class="left_td">操作：</td>
        <td>@Html.DisplayText("ReType")操作</td>
    </tr>
    <tr>
        <td class="left_td">退换理由：</td>
        <td>@Html.DisplayText("ReReason")</td>
        <td class="left_td">退换数量：</td>
        <td>@Html.DisplayText("ReturnNum")@Html.DisplayText("SubUnit")</td>
    </tr>

</table>
