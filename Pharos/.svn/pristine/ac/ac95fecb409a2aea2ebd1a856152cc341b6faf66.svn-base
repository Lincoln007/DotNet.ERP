using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace Pharos.Logic.Entity
{
    /// <summary>
    /// 产品变价
    /// </summary>
    public class ProductChangePrice
    {
        public string Id { get; set; }
        /// <summary>
        /// 供应商
        /// </summary>
        public string SupplierId { get; set; }
        /// <summary>
        /// 调价门店，多个以逗号隔开
        /// </summary>
        public string StoreId { get; set; }
        /// <summary>
        /// 录入人
        /// </summary>
        public string CreateUID { get; set; }
        /// <summary>
        /// 审批人
        /// </summary>
        public string AuditorUID { get; set; }
        /// <summary>
        /// 创建时间
        /// </summary>
        public DateTime CreateDT { get; set; }
        /// <summary>
        /// 状态 0-未审批1-已审批2-已失效
        /// </summary>
        public short State { get; set; }
        /// <summary>
        /// 审批时间
        /// </summary>
        public DateTime? AuditorDT { get; set; }
        public List<ProductChangePriceList> ChangePriceList { get; set; }
    }
    /// <summary>
    /// 变价子表
    /// </summary>
    public class ProductChangePriceList
    {
        public int Id { get; set; }
        public string ChangePriceId { get; set; }
        public string Barcode { get; set; }
        /// <summary>
        /// 原进价
        /// </summary>
        public decimal OldBuyPrice { get; set; }
        /// <summary>
        /// 现进价
        /// </summary>
        public decimal CurBuyPrice { get; set; }
        /// <summary>
        /// 原售价
        /// </summary>
        public decimal OldSysPrice { get; set; }
        /// <summary>
        /// 现售价
        /// </summary>
        public decimal CurSysPrice { get; set; }
        /// <summary>
        ///原毛利率（%）
        /// </summary>
        public decimal OldGrossprofitRate { get; set; }
        /// <summary>
        /// 现毛利率（%）
        /// </summary>
        public decimal CurGrossprofitRate { get; set; }
        /// <summary>
        /// 开始时间
        /// </summary>
        public string StartDate { get; set; }
        /// <summary>
        /// 结束时间 NULL-不限
        /// </summary>
        public string EndDate { get; set; }
        public string Memo { get; set; }
        /// <summary>
        /// 状态（ 0:无效、 1:有效）
        /// </summary>
        public short State { get; set; }
    }
}
