@model Pharos.Sys.Entity.OMS_CompanyAuthorize
@{
    ViewBag.Title = "Save";
    Layout = "~/Views/Shared/_FormLayout.cshtml";
}
<div class="default-form">
    @using(Html.BeginForm())
    {
        <div class="content">
            <table class="table-form" width="100%" height="320px">
                <tr>
                    <td class="name">申请类别：</td>
                    <td class="input">@Html.RadioButtonListFor(o => o.Category, new List<SelectListItem>() { new SelectListItem() { Text = "在线", Value = "1" }, new SelectListItem() { Text = "独立", Value = "2" } })</td>
                </tr>
                <tr>
                    <td class="name">单位简称：</td>
                    <td class="input">@Html.TextBoxFor(o => o.Title, new { @class = "easyui-textbox", data_options = "novalidate:true,required:true" })</td>
                </tr>
                <tr>
                    <td class="name">单位全称：</td>
                    <td class="input">@Html.TextBoxFor(o => o.FullTitle, new { @class = "easyui-textbox", data_options = "novalidate:true,required:true" })</td>
                </tr>
                <tr>
                    <td class="name">联系人：</td>
                    <td class="input">@Html.TextBoxFor(o => o.LinkMan, new { @class = "easyui-textbox" })</td>
                </tr>
                <tr>
                    <td class="name">联系电话：</td>
                    <td class="input">@Html.TextBoxFor(o => o.Phone, new { @class = "easyui-textbox" })</td>
                </tr>
                <tr>
                    <td class="name">用户数量：</td>
                    <td class="input">@Html.TextBoxFor(o => o.UserNum, new { @class = "easyui-numberbox", data_options = "precision:0" })</td>
                </tr>
                <tr>
                    <td class="name">门店数量：</td>
                    <td class="input">@Html.TextBoxFor(o => o.StoreNum, new { @class = "easyui-numberbox", data_options = "precision:0" })</td>
                </tr>
                <tr>
                    <td></td>
                    <td><a class="easyui-linkbutton cus4 linkbtn btn" href="#" onclick="$('form').submit()">发送申请</a></td>
                </tr>
            </table>
        </div>
    }
</div>